	Mesh = {Alternative Splicing; Base Sequence; Cell Line; Exons; Gene Expression Profiling; Humans; Open Reading Frames; Organ Specificity; Polyadenylation; Protein Isoforms; RNA, Messenger; RNA-Binding Proteins; Repressor Proteins},
