	Doi = {10.1161/ATVBAHA.107.159228},
	Journal = {Arterioscler Thromb Vasc Biol},
	Journal-Full = {Arteriosclerosis, thrombosis, and vascular biology},
	Mesh = {Cardiovascular Diseases; Humans; Insulin Resistance; Intra-Abdominal Fat; Metabolic Syndrome X; Obesity; Risk Factors; Triglycerides},
	Month = {Jun},
	Number = {6},
	Pages = {1039-49},
