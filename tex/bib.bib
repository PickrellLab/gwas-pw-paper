	Number = {8},
	Pages = {1877-88},
	Pmc = {PMC3708501},
	Pmid = {23666210},
	Pst = {ppublish},
	Title = {Genetic signatures reveal high-altitude adaptation in a set of ethiopian populations},
	Volume = {30},
	Year = {2013},
