	Journal = {Am J Hum Genet},
	Journal-Full = {American journal of human genetics},
	Mesh = {Asian Continental Ancestry Group; China; Chromosome Mapping; Chromosomes, Human, Pair 21; Ethnic Groups; European Continental Ancestry Group; Genome, Human; Heterozygote; Humans; Linkage Disequilibrium; Phylogeny; Polymorphism, Single Nucleotide},
