	Journal = {PLoS Genet},
	Journal-Full = {PLoS genetics},
	Mesh = {Genome, Human; Genome-Wide Association Study; Humans; Phenotype; Polymorphism, Single Nucleotide; Quantitative Trait Loci},
	Month = {Apr},
	Number = {4},
	Pages = {e1000888},
	Pmc = {PMC2848547},
	Pmid = {20369019},
	Pst = {epublish},
	Title = {Trait-associated {SNPs} are more likely to be {eQTLs}: annotation to enhance discovery from {GWAS}},
