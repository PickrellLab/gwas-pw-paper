	Mesh = {Algorithms; Alternative Splicing; Computational Biology; Embryonic Stem Cells; Exons; Expressed Sequence Tags; False Positive Reactions; Gene Expression Regulation; Humans; Models, Neurological; Nervous System; Nervous System Physiological Phenomena; Neurons; Reverse Transcriptase Polymerase Chain Reaction; Stem Cells},
	Month = {Oct},
