	Pages = {f4262},
	Pmc = {PMC3715134},
	Pmid = {23869090},
	Pst = {epublish},
	Title = {Association of plasma uric acid with ischaemic heart disease and blood pressure: mendelian randomisation analysis of two large cohorts},
