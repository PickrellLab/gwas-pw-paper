	Journal = {Blood},
	Journal-Full = {Blood},
	Mesh = {Base Sequence; Cohort Studies; Genotype; Haplotypes; Humans; Middle Aged; Molecular Sequence Data; Polymorphism, Single Nucleotide; von Willebrand Factor},
