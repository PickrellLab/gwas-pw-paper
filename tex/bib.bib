	Mesh = {Animals; Animals, Domestic; Animals, Wild; Breeding; Computational Biology; Dogs; Evolution, Molecular; Far East; Genome; Haplotypes; Middle East; Phenotype; Phylogeny; Polymorphism, Single Nucleotide; Wolves},
	Month = {Apr},
