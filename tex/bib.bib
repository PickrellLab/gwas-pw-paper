	Mesh = {Alleles; Chromosome Mapping; Computer Simulation; Female; Gene Frequency; Genetic Diseases, Inborn; Genetic Heterogeneity; Genetic Linkage; Genetic Predisposition to Disease; Genetic Variation; Humans; Male; Models, Genetic; Mutagenesis; Mutation; Pedigree; Phenotype; Polymorphism, Genetic; Probability; Selection, Genetic; Software},
	Month = {Jul},
	Number = {1},
	Pages = {124-37},
