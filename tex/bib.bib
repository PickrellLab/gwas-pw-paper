	Doi = {10.1016/j.ajhg.2012.01.002},
	Journal = {Am J Hum Genet},
	Journal-Full = {American journal of human genetics},
	Mesh = {Base Sequence; DNA, Mitochondrial; Ethnic Groups; European Continental Ancestry Group; Gene Frequency; Genetic Variation; Genetics, Population; Haplotypes; Humans; Molecular Sequence Data; Phylogeny},
	Month = {Mar},
	Number = {3},
	Pages = {486-93},
	Pmc = {PMC3309182},
	Pmid = {22365151},
	Pst = {ppublish},
	Title = {The Basque paradigm: genetic evidence of a maternal continuity in the Franco-Cantabrian region since pre-Neolithic times},
	Volume = {90},
	Year = {2012},
