	Mesh = {Cardiovascular Diseases; Genetic Loci; Genetic Pleiotropy; Genetic Predisposition to Disease; Genome-Wide Association Study; Humans; Phenotype; Polymorphism, Single Nucleotide; Reproducibility of Results; Risk Factors; Schizophrenia},
	Month = {Feb},
