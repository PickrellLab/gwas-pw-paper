@article{Rasmussen:2011fk,
	Abstract = {We present an Aboriginal Australian genomic sequence obtained from a 100-year-old lock of hair donated by an Aboriginal man from southern Western Australia in the early 20th century. We detect no evidence of European admixture and estimate contamination levels to be below 0.5%. We show that Aboriginal Australians are descendants of an early human dispersal into eastern Asia, possibly 62,000 to 75,000 years ago. This dispersal is separate from the one that gave rise to modern Asians 25,000 to 38,000 years ago. We also find evidence of gene flow between populations of the two dispersal waves prior to the divergence of Native Americans from modern Asian ancestors. Our findings support the hypothesis that present-day Aboriginal Australians descend from the earliest humans to occupy Australia, likely representing one of the oldest continuous populations outside Africa.},
	Author = {Rasmussen, Morten and Guo, Xiaosen and Wang, Yong and Lohmueller, Kirk E and Rasmussen, Simon and Albrechtsen, Anders and Skotte, Line and Lindgreen, Stinus and Metspalu, Mait and Jombart, Thibaut and Kivisild, Toomas and Zhai, Weiwei and Eriksson, Anders and Manica, Andrea and Orlando, Ludovic and De La Vega, Francisco M and Tridico, Silvana and Metspalu, Ene and Nielsen, Kasper and {\'A}vila-Arcos, Mar{\'\i}a C and Moreno-Mayar, J V{\'\i}ctor and Muller, Craig and Dortch, Joe and Gilbert, M Thomas P and Lund, Ole and Wesolowska, Agata and Karmin, Monika and Weinert, Lucy A and Wang, Bo and Li, Jun and Tai, Shuaishuai and Xiao, Fei and Hanihara, Tsunehiko and van Driem, George and Jha, Aashish R and Ricaut, Fran{\c c}ois-Xavier and de Knijff, Peter and Migliano, Andrea B and Gallego Romero, Irene and Kristiansen, Karsten and Lambert, David M and Brunak, S{\o}ren and Forster, Peter and Brinkmann, Bernd and Nehlich, Olaf and Bunce, Michael and Richards, Michael and Gupta, Ramneek and Bustamante, Carlos D and Krogh, Anders and Foley, Robert A and Lahr, Marta M and Balloux, Francois and Sicheritz-Pont{\'e}n, Thomas and Villems, Richard and Nielsen, Rasmus and Wang, Jun and Willerslev, Eske},
