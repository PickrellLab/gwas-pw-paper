	Doi = {10.1001/jama.2010.840},
	Journal = {JAMA},
	Journal-Full = {JAMA},
	Mesh = {Aged; Female; Folic Acid; Homocysteine; Humans; Hyperhomocysteinemia; Male; Middle Aged; Morbidity; Myocardial Infarction; Treatment Outcome; Vitamin B 12; Vitamin B Complex},
	Month = {Jun},
	Number = {24},
