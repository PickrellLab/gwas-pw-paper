	Journal-Full = {Journal of human evolution},
	Mesh = {Adaptation, Physiological; Animals; Biological Evolution; Cholecalciferol; Emigration and Immigration; Environmental Exposure; Female; Folic Acid; Hominidae; Humans; Models, Biological; Pregnancy; Reproduction; Skin Pigmentation; Ultraviolet Rays},
	Month = {Jul},
