	Date-Modified = {2009-12-02 19:01:48 -0600},
	Doi = {10.1073/pnas.0904715106},
	Journal = {Proc Natl Acad Sci U S A},
	Journal-Full = {Proceedings of the National Academy of Sciences of the United States of America},
	Mesh = {Chromatin; Epigenesis, Genetic; Gene Expression Profiling; Gene Expression Regulation; Humans; Models, Genetic; Oligonucleotide Array Sequence Analysis; RNA, Untranslated; Repressor Proteins},
	Month = {Jul},
	Number = {28},
