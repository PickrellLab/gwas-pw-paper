	Journal = {Eur J Cardiovasc Prev Rehabil},
	Journal-Full = {European journal of cardiovascular prevention and rehabilitation : official journal of the European Society of Cardiology, Working Groups on Epidemiology \& Prevention and Cardiac Rehabilitation and Exercise Physiology},
	Mesh = {Aged; Cholesterol, HDL; Coronary Artery Disease; Dietary Supplements; Disease Progression; Energy Intake; Fatty Acids, Omega-3; Humans; Hypercholesterolemia; Male; Middle Aged; Multivariate Analysis; Tunica Intima},
