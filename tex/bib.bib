	Journal = {PLoS Genet},
	Journal-Full = {PLoS genetics},
	Mesh = {Animals; Chromosome Mapping; Exons; Gene Expression Regulation; Genetic Variation; Male; Mice; Models, Genetic; Muscle, Skeletal; Organ Specificity; Prostate; Transcription, Genetic},
