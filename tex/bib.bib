	Mesh = {Genome-Wide Association Study; Models, Theoretical; Phenotype},
	Number = {5},
	Pages = {e34861},
	Pmc = {PMC3342314},
	Pmid = {22567092},
	Pst = {ppublish},
	Title = {MultiPhen: joint model of multiple phenotypes can increase discovery in GWAS},
