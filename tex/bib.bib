	Mesh = {Base Sequence; Bias (Epidemiology); Gene Expression Profiling; Humans; Oligonucleotide Array Sequence Analysis; RNA, Messenger; Systems Biology; Transcription, Genetic},
	Pages = {14},
	Pmid = {19371405},
	Title = {Transcript length bias in RNA-seq data confounds systems biology},
