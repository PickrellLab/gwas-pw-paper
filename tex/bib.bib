	Journal = {PLoS Genet},
	Journal-Full = {PLoS genetics},
	Mesh = {Algorithms; Computational Biology; Databases, Nucleic Acid; Gene Regulatory Networks; Humans; Oligonucleotide Array Sequence Analysis; Polymorphism, Single Nucleotide; Quantitative Trait Loci; Software; Yeasts},
	Month = {Jan},
	Number = {1},
	Pages = {e1000358},
