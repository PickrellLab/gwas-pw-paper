	Journal = {J Clin Endocrinol Metab},
	Journal-Full = {The Journal of clinical endocrinology and metabolism},
	Mesh = {Adiposity; Aged; Aged, 80 and over; Biological Markers; C-Reactive Protein; Female; Genetic Linkage; Genetic Markers; Genotype; Humans; Inflammation; Male; Mendelian Randomization Analysis; Polymorphism, Single Nucleotide; Proteins; Receptor, Melanocortin, Type 4},
