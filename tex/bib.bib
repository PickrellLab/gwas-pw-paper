	Number = {7095},
	Pages = {840-6},
	Pmid = {16699522},
	Pst = {ppublish},
	Title = {Single-cell proteomic analysis of S. cerevisiae reveals the architecture of biological noise},
	Volume = {441},
	Year = {2006},
