	Mesh = {Alleles; Cluster Analysis; Gene Frequency; Genetics, Population; Genome, Human; Genotype; Geography; Humans; Models, Genetic; Models, Statistical; Multigene Family; Population Groups},
	Month = {Dec},
	Number = {6},
