	Mesh = {Aged; Aged, 80 and over; Aging; Chronic Disease; Genome-Wide Association Study; Humans},
	Month = {Oct},
	Number = {5},
	Pages = {727-31},
	Pmc = {PMC3444649},
	Pmid = {22888763},
	Pst = {ppublish},
	Title = {Review: a meta-analysis of GWAS and age-associated diseases},
