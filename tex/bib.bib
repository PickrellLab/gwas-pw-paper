	Doi = {10.1126/scitranslmed.3007191},
	Journal = {Sci Transl Med},
	Journal-Full = {Science translational medicine},
	Month = {Apr},
	Number = {234},
	Pages = {234ra57},
	Pmid = {24786325},
	Pst = {ppublish},
	Title = {Disease risk factors identified through shared genetic architecture and electronic medical records},
	Volume = {6},
	Year = {2014},
