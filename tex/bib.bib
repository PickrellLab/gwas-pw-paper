	Doi = {10.1101/gr.087577.108},
	Journal = {Genome Res},
	Journal-Full = {Genome research},
	Mesh = {Diabetes Mellitus, Type 2; Genetic Predisposition to Disease; Genetics, Population; Genome, Human; Genome-Wide Association Study; Humans; Neuregulins; Phenotype; Receptor, Epidermal Growth Factor; Selection, Genetic},
	Month = {May},
