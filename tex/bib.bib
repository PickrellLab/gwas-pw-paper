	Doi = {10.1261/rna.151106},
	Journal = {RNA},
	Journal-Full = {RNA (New York, N.Y.)},
	Mesh = {Alternative Splicing; Codon; Computational Biology; Databases, Genetic; Genome, Human; Humans; RNA Precursors; RNA, Messenger},
